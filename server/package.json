    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nock": "^12.0.3",
    "nodemon": "^2.0.4",
    "serialport": "^8.0.8",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
